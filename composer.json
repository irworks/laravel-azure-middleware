{
    "name": "rootinc/laravel-azure-middleware",
    "description": "Azure Middleware Auth",
    "type": "library",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.4.0",
        "guzzlehttp/guzzle": ">=6.2",
        "microsoft/microsoft-graph": "dev-dev#6f79a9d"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:irworks/msgraph-sdk-php.git"
        }
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Root Inc",
            "email": "djewett@rootinc.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RootInc\\LaravelAzureMiddleware\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RootInc\\LaravelAzureMiddleware\\AzureServiceProvider"
            ]
        }
    }
}